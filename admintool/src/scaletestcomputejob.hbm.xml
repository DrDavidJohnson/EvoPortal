<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="uk.ac.rdg.evoportal.beans.ScaleTestComputeJob" table="ScaleTestComputeJob">
    <id column="scaletestcomputejob_id" name="id">
      <generator class="native"/>
    </id>
    <property name="jobID"/>
    <property name="label"/>
    <property name="nodes"/>
    <property name="submitTime"/>
    <property name="status"/>
    <property name="timeRequested"/>
    <property name="timeUsed"/>
    <property name="owner"/>
    <property name="duration"/>
    <property name="scaletest_id"/>
  </class>
</hibernate-mapping>
